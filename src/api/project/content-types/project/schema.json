{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Project",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 255
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": false,
      "maxLength": 255
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "projects"
    },
    "brands": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::brand.brand",
      "inversedBy": "projects"
    },
    "designer": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::designer.designer"
    },
    "hero": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "card": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "intro": {
      "type": "component",
      "repeatable": false,
      "component": "project-blades.intro",
      "required": true
    },
    "projectCta": {
      "type": "component",
      "repeatable": false,
      "component": "page-blades.content-cta"
    },
    "projectHero": {
      "type": "component",
      "repeatable": false,
      "component": "page-blades.hero"
    },
    "projectDynamicContent": {
      "type": "dynamiczone",
      "components": [
        "project-blades.hero",
        "page-blades.quote",
        "page-blades.rich-text"
      ]
    }
  }
}
